{
  "table": {
    "border-collapse": "collapse",
    "margin-bottom": "2em"
  },
  "th": {
    "text-align": "left",
    "vertical-align": "top",
    "padding": "3px 5px",
    "border": "1px solid #ccc"
  },
  "td": {
    "vertical-align": "top",
    "padding": "3px 5px",
    "border": "1px solid #ccc"
  },
  "td.too-many-rows": {
    "color": "red",
    "font-size": "smaller"
  }
}
