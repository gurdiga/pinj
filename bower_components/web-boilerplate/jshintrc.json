{
  "bitwise": true,
  "camelcase": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "noarg": true,
  "noempty": true,
  "quotmark": true,
  "undef": true,
  "unused": true,
  "trailing": true,
  "maxdepth": 4,
  "maxcomplexity": 4,
  "white": false,
  "sub": true,
  "evil": true,
  "strict": true,
  "-W030": true,

  "globals": {
    "window": false,
    "document": false,
    "location": false,
    "setInterval": false,
    "setTimeout": false,
    "clearTimeout": false,
    "console": false,

    "describe": false,
    "it": false,
    "expect": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "sinon": false
  }
}
